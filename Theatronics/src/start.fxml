<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="hBoxMain"  alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="startcntrl">

    <Button minHeight="600.0" minWidth="333.0" fx:id="btn_mngr" onAction="#onmngrpress" onMouseEntered="#cntrl" onMouseExited="#rel" style="-fx-font-size: 18px; -fx-background-color: #DDDDDD">
        Admin
    </Button>
    <Button minHeight="600.0" minWidth="333.0" fx:id="btn_cshr" onAction="#oncashpress" onMouseEntered="#cntrl2" onMouseExited="#rel2" style="-fx-font-size: 18px; -fx-background-color: #EEEEEE">
        Cashier
    </Button>
    <Button minHeight="600.0" minWidth="333.0" fx:id="btn_user" onAction="#onuserpress" onMouseEntered="#cntrl3" onMouseExited="#rel3" style="-fx-font-size: 18px; -fx-background-color: #FFFFFF">
        User
    </Button>
</HBox>
